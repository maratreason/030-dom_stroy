extends pug/base.pug

append content
  include pug/includes/header.pug

  main.main
    section.subcatalog
      .container
        +breadcrumbs("Главная", "Каталог", "Каркасные дома", "Каркасно-модульный дом Венеция")


    section.subcatalog-main
      .container
        .subcatalog-main__inner
          .subcatalog-main__slider(data-subcatalog-slider)
            img.subcatalog-main__slider-big(src="images/subcatalog-details/big-img.jpg" alt="" data-picture-big)
            .subcatalog-main__slider-swiper.swiper
              ul.subcatalog-main__slider-list.swiper-wrapper
                li.subcatalog-main__slider-item.swiper-slide(data-picture-small)
                  img.subcatalog-main__slider-img(src="images/subcatalog-details/item-1.jpg" alt="")
                li.subcatalog-main__slider-item.swiper-slide(data-picture-small)
                  img.subcatalog-main__slider-img(src="images/subcatalog-details/item-2.jpg" alt="")
                li.subcatalog-main__slider-item.swiper-slide(data-picture-small)
                  img.subcatalog-main__slider-img(src="images/subcatalog-details/item-3.jpg" alt="")
                li.subcatalog-main__slider-item.swiper-slide(data-picture-small)
                  img.subcatalog-main__slider-img(src="images/subcatalog-details/item-4.jpg" alt="")
                li.subcatalog-main__slider-item.swiper-slide(data-picture-small)
                  img.subcatalog-main__slider-img(src="images/subcatalog-details/item-1.jpg" alt="")
                li.subcatalog-main__slider-item.swiper-slide(data-picture-small)
                  img.subcatalog-main__slider-img(src="images/subcatalog-details/item-3.jpg" alt="")
            .subcatalog-main__slider-btns
              button.subcatalog-main-slider__btn.subcatalog-main__btn--left
                img(src="images/icons/middle-chevron-left.svg" alt="")
              button.subcatalog-main-slider__btn.subcatalog-main__btn--right
                img(src="images/icons/middle-chevron-right.svg" alt="")

          .subcatalog-main__content
            h1.subcatalog-main__title Каркасно-модульный дом Венеция 4x4,6 м с террасой, кухней и спальней
            p.
              Недорогой дачный дом размером 6х8м. с панорамным остеклением,
              возможностью выбора с сезонного проживания на постоянное.
            ul.subcatalog-main__content-list.subcatalog-list
              li.subcatalog-item
                .subcatalog-item__block
                  img.subcatalog-item__img(src="images/icons/meters.svg" alt="meters icon")
                  .subcatalog-item__text Размер дома
                .subcatalog-item__block
                  .subcatalog-item__boldtext 4x6,9 м
              li.subcatalog-item
                .subcatalog-item__block
                  img.subcatalog-item__img(src="images/icons/square.svg" alt="square icon")
                  .subcatalog-item__text Площадь застройки
                .subcatalog-item__block
                  .subcatalog-item__boldtext 30 м2
              li.subcatalog-item
                .subcatalog-item__block
                  img.subcatalog-item__img(src="images/icons/clock.svg" alt="clock icon")
                  .subcatalog-item__text Срок строительства
                .subcatalog-item__block
                  .subcatalog-item__boldtext 20 дней

            button.subcatalog-main__btn.btn.btn-warning.btn-wide Задать вопрос

    section.subcatalog-info
      .container
        .subcatalog-info__inner
          ul.subcatalog-info__accordion(data-accordion-info)
            li.subcatalog-info__item(data-accordion-title-id="1")
              .subcatalog-info__block
                label.checkbox__label(for="stage-1")
                  input.checkbox(type="checkbox" id="stage-1")
                  span.checkbox__fake
                  span.checkbox__title Без внутренней отделки
                  .subcatalog-info__box
                    span.checkbox__title 550 руб
            li.subcatalog-info__item(data-accordion-title-id="2")
